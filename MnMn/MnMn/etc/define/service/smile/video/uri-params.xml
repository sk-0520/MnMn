<?xml version="1.0" encoding="utf-8" ?>
<parameters>
	<param key="video-ranking">
		<!--ターゲット-->
		<item key="" value="${target}" prefix="/" />
		<!--期間-->
		<item key="" value="${period}" prefix="/" />
		<!--category-->
		<item key="" value="${category}" prefix="/" />
		<item prefix="?" key="rss" bond="=" value="2.0" />
		<item prefix="&amp;" key="lang" bond="=" value="${lang}" />
	</param>
	<param key="video-getthumbinfo">
		<item key="video-id" value="${video-id}" />
	</param>
	<param key="video-getflv-normal">
		<item key="video-id" value="${video-id}" />
	</param>
	<param key="video-getflv-swf">
		<item key="video-id" value="${video-id}" />
		<item prefix="?" key="as3" bond="=" value="1" />
	</param>
	<param key="video-getthreadkey">
		<item key="thread" value="${thread-id}" />
	</param>

	<param key="video-newarrival">
		<item key="rss" value="2.0" />
		<item key="lang" value="${lang}" />
	</param>
	<param key="video-recent">
		<item key="rss" value="2.0" />
		<item key="lang" value="${lang}" />
	</param>
	
</parameters>