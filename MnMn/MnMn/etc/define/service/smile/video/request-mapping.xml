<?xml version="1.0" encoding="utf-8" ?>
<mappings>
	<mapping key="video-msg">
		<item key="thread-id" type="simple" value="${thread-id}"  />
		<item key="version"   type="simple" value="20061206"  />
		<item key="res_from"  type="simple" value="${res_from}"  />
		<item key="user-id"   type="force-pair" name="user_id" bond="=" value="${user-id}" fail="">
			<bracket target="value" open="&quot;" close="&quot;" />
		</item>
		<item key="threadkey" type="force-pair" name="threadkey" bond="=" value="${threadkey}" fail="" >
			<bracket target="value" open="&quot;" close="&quot;" />
		</item>
		<item key="force_184" type="force-pair" name="force_184" bond="=" value="${force_184}" fail="">
			<bracket target="value" open="&quot;" close="&quot;" />
		</item>
		
		<item key="time-msg" type="force-pair" name="${time-size}" bond="," value="${all-size}" fail="" />

		<content trim="content" oneline="true">
			<![CDATA[
				<packet>
					<thread thread="${thread-id}" scores="1" version="${version}" res_from="${res_from}" ${user-id} ${threadkey} ${force_184} />
					<thread_leaves thread="${thread-id}" >${time-msg}</thread_leaves>
					<thread thread="${thread-id}" version="${version}" res_from="${res_from}" ${threadkey} ${force_184} fork="1"/>
				</packet>
			]]>
		</content>
		<!--
					<thread thread="${thread-id}" version="${version}" scores="(0|1)" nicoru="(0|1)" language="(0|1|2)" with_global="(0|1)"/>
					<thread_leaves thread="${thread-id}" scores="(0|1)" nicoru="(0|1)" language="(0|1|2)">time-msg</thread_leaves>
					<thread thread="${thread-id}" version="${version}" res_from="-1000" fork="1"/>
		-->
	</mapping>
</mappings>
