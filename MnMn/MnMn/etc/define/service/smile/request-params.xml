<?xml version="1.0" encoding="utf-8" ?>
<parameters>
	<param key="video-session-login" type="Text">
		<item key="mail_tel" value="${user}" />
		<item key="password" value="${pass}" />
		<item key="next_url" value="" />
	</param>
	<param key="smile-video-mylist-account-default-add" type="Text">
		<item key="item_id" value="${video-id}" />
		<item key="token" value="${token}" />
		<item key="unwatched" value="0" />
	</param>
	<param key="smile-video-mylist-account-add" type="Text">
		<item key="group_id" value="${mylist-id}" />
		<item key="item_id" value="${thread-id}" />
		<item key="token" value="${token}" />
		<item key="unwatched" value="0" />
	</param>
	<param key="smile-video-mylist-account-group-del" type="Text">
		<item key="group_id" value="${mylist-id}" />
		<item key="token" value="${token}" />
	</param>
	<param key="smile-video-mylist-account-group-update" type="Text">
		<item key="group_id" value="${mylist-id}" />
		<item key="token" value="${token}" />
		<item key="name" value="${name}" />
		<item key="description" value="${description}" />
		<item key="public" value="${public}" />
		<item key="default_sort" value="${sort}" />
		<item key="icon_id" value="${folder-id}" />
	</param>
</parameters>
