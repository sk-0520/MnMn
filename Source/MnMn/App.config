		<add key="setting-service-smile-smilevideo-player-show-post-timestamp" value="false" />
		<add key="setting-service-smile-smilevideo-player-comment-visible" value="true" />
		<add key="setting-service-smile-smilevideo-player-global-comment-filtering-is-enabled" value="true" />

		<add key="setting-service-smile-smilevideo-filtering-ignore-overlap-word" value="false" />
		<add key="setting-service-smile-smilevideo-filtering-ignore-overlap-time" value="00:00:03" />

	</appSettings>
</configuration>
