<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
	</startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<probing privatePath="lib"/>
		</assemblyBinding>
	</runtime>
	<appSettings>
		<add key="app-uri-about" value="http://content-type-text.net/"/>
		<add key="app-mail-about" value="content.type.text.net@gmail.com"/>
		<add key="app-uri-development" value="https://bitbucket.org/sk_0520/mnmn"/>
		<add key="app-uri-forum" value="https://groups.google.com/d/forum/mnmn-forum"/>
		<add key="app-uri-update" value="https://bitbucket.org/sk_0520/mnmn/downloads/update.xml?{TIMESTAMP}"/>
		<add key="app-uri-changelog-release" value="https://bitbucket.org/sk_0520/mnmn/downloads/update-release.html?{TIMESTAMP}"/>
		<!--<add key="app-uri-changelog-rc" value="http://" />-->
		<add key="app-uri-issue-resolved" value="https://bitbucket.org/sk_0520/mnmn/issues?status=resolved"/>

		<add key="backup-archive-count" value="5" />
		<add key="backup-setting-count" value="30"/>
		<add key="update-app-exit-wait-time" value="00:30:00" />

		<add key="background-auto-save-setting-time" value="00:40:00" />
		<add key="background-update-check-time" value="01:30:00" />
		<add key="background-garbage-collection-is-enabled-startup" value="true" />
		<add key="background-garbage-collection-time" value="02:00:00" />
		<add key="background-garbage-collection-level" value="Large, Temporary" />

		<add key="log-view-count" value="5000" />
		<add key="text-file-save-buffer" value="4096" />
		<add key="player-cursor-hide-time" value="00:00:03" />

		<add key="setting-application-cache-life-time" value="3.00:00:00" />

		<add key="setting-application-window-left" value="100" />
		<add key="setting-application-window-top" value="100" />
		<add key="setting-application-window-width" value="720" />
		<add key="setting-application-window-height" value="480" />

		<!-- ニコニコ動画 ======================================== -->
		<add key="service-smile-account-register" value="https://account.nicovideo.jp/register?from=top" />
		<add key="service-smile-account-reset" value="https://account.nicovideo.jp/password/reset" />

		<add key="service-smile-content_search-context" value="MnMn" />
		<add key="service-smile-mylist-reload-wait-time" value="00:00:00.5" />
		<add key="service-smile-mylist-folder-color" value="#ff87ceeb" />
		<add key="service-smile-mylist-history-count" value="50" />

		<add key="service-smile-smilevideo-search-history-count" value="100" />
		<add key="service-smile-smilevideo-is-enabled-filtering" value="true" />
		<add key="service-smile-smilevideo-play-history-count" value="500" />
		<add key="service-smile-smilevideo-check_it_later-count" value="200" />
		<add key="service-smile-smilevideo-downloading_error-wait-time" value="00:00:01" />
		<add key="service-smile-smilevideo-downloading_error-retry-count" value="5" />
		<add key="service-smile-smilevideo-watch-to-movie-wait-time" value="00:00:01" />
		<add key="service-smile-smilevideo-play-lowest-size" value="2097152" />
		<add key="service-smile-smilevideo-receive-buffer" value="67108864" />
