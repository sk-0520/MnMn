		<add key="service-smile-smilevideo-check_it_later-check-time" value="03:00:00" />
		<add key="service-smile-smilevideo-comment-correction-time" value="00:00:01" />
		<add key="service-smile-smilevideo-comment-simple-newline" value="˽" />
		<add key="service-smile-smilevideo-comment-simple-space" value="↵" />
		<add key="service-smile-relation-video-sort" value="p" />
		<add key="service-smile-relation-video-order-ascending" value="a" />
		<add key="service-smile-relation-video-order-descending" value="d" />

