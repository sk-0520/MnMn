		<add key="setting-service-smile-smilevideo-comment-font-bold" value="false" />
		<add key="setting-service-smile-smilevideo-comment-font-italic" value="false" />
		<add key="setting-service-smile-smilevideo-comment-show-time" value="00:00:03" />
