<?xml version="1.0" encoding="utf-8" ?>
<web-navigator>
	<navigating>
		<element key="">
			<extends>
				<extend key="" value="" />
			</extends>
		</element>
	</navigating>
	<click>
		<element key="">
			<extends>
				<extend key="" value="" />
			</extends>
		</element>
	</click>
	<context-menu>
		<element key="common-back" separator="false" allow-service="*" send-service="common">
			<words>
				<word key="ja-jp" value="戻る" />
			</words>
		</element>
		<element key="smile-video--open-player" separator="false" allow-service="*" send-service="smile-video">
			<words>
				<word key="ja-jp" value="プレイヤーで開く" />
			</words>
			<conditions >
				<condition visible="true">
					<base>//www\.nicovideo\.jp</base>
					<targets>
						<target tag=".*/a" attribute="href" value="\.*/watach/[\w]+" />
					</targets>
					<parameter tag=".*/a" attribute="href" value="\.*/watach/(?&lt;PARAM&gt;[\w])+" param="${PARAM}" />
				</condition>
			</conditions>
		</element>
	</context-menu>
</web-navigator>