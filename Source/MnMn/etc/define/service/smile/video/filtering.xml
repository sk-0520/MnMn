<?xml version="1.0" encoding="utf-8" ?>
<filtering>
	<element key="lol">
		<words>
			<word key="ja-jp" value="笑ってるだけのコメント" />
		</words>
		<extends>
			<extend key="pattern" value="^\s*[wWｗＷ草][wWｗＷ草\s]*$" />
			<extend key="ignore-case" value="false" />
		</extends>
	</element>
	<element key="sympathy">
		<words>
			<word key="ja-jp" value="投稿者への労り" />
		</words>
		<extends>
			<extend key="pattern" value="(うぽ(つ|っ))|(う(ｐ|p)乙)|(^[8８]+$)" />
			<extend key="ignore-case" value="false" />
		</extends>
	</element>
	<element key="sense-of-unity">
		<words>
			<word key="ja-jp" value="すごい一体感を感じる" />
		</words>
		<extends>
			<extend key="pattern" value="(ざわ)|(^(来|く)るぞ)|(鳥\s*肌)|(^あっ?(…|・)*?$)|(^[！？!\?)]+$)" />
			<extend key="ignore-case" value="false" />
		</extends>
	</element>
	<element key="chat">
		<words>
			<word key="ja-jp" value="話されても困る" />
		</words>
		<extends>
			<extend key="pattern" value="(^[↑↓←→])|(.+→)|(挙手)|(^ノ|ﾉｼ?$)" />
			<extend key="ignore-case" value="false" />
		</extends>
	</element>
	<element key="command">
		<words>
			<word key="ja-jp" value="無意味なコマンド的な何か" />
		</words>
		<extends>
			<extend key="pattern" value="(^eco=)|(/ksk)|(/hidden)|(@|＠倍速)|(/逆再生)" />
			<extend key="ignore-case" value="false" />
		</extends>
	</element>
	<element key="kill-lgbt">
		<words>
			<word key="ja-jp" value="殺せ、同性愛者だ" />
		</words>
		<extends>
			<extend key="pattern" value="(ん([\?？]))|((アッー)|(ｱｯｰ))|((今|いま)(なんでも))|(あくしろよ)|(あら＾)|(\w+([\(（].+[\)）]?))|(ファッ！？)|(説$)|(大胆な)" />
			<extend key="ignore-case" value="true" />
		</extends>
	</element>
	<element key="kiddie">
		<words>
			<word key="ja-jp" value="話されても困る" />
		</words>
		<extends>
			<extend key="pattern" value="(^device:(3DS|WiiU)$)" />
			<extend key="ignore-case" value="true" />
		</extends>
	</element>
</filtering>