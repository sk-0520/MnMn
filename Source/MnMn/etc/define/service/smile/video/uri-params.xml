<?xml version="1.0" encoding="utf-8" ?>
<parameters>
	<param key="video-ranking">
		<!--ターゲット-->
		<item key="" value="${target}" prefix="/" />
		<!--期間-->
		<item key="" value="${period}" prefix="/" />
		<!--category-->
		<item key="" value="${category}" prefix="/" />
		<item prefix="?" key="rss" bond="=" value="2.0" />
		<item prefix="&amp;" key="lang" bond="=" value="${lang}" />
	</param>
	<param key="video-getthumbinfo">
		<item key="video-id" value="${video-id}" />
	</param>
	<param key="video-getflv-normal">
		<item key="video-id" value="${video-id}" />
	</param>
	<param key="video-getthreadkey">
		<item key="thread" value="${thread-id}" />
	</param>

	<param key="video-newarrival">
		<item key="rss" value="2.0" />
		<item key="lang" value="${lang}" />
	</param>
	<param key="video-recent">
		<item key="rss" value="2.0" />
		<item key="lang" value="${lang}" />
	</param>

	<param key="video-getrelation">
		<item key="page" value="${page}" />
		<item key="sort" value="${sort}" />
		<item key="order" value="${order}" />
		<item key="video" value="${video-id}" />
	</param>

	<param key="video-tag-feed">
		<item key="rss" value="2.0" />
		<item key="page" value="${page}" force="true" />
		<item key="sort" value="${sort}" force="true" />
		<item key="order" value="${order}" force="true" />
	</param>

	<param key="video-recommendation-page">
		<item key="ref" value="top_recoall" />
	</param>
	<param key="video-recommendation-api">
		<item key="user_tags" value="${user-tags}" />
		<item key="seed" value="${seed}" />
		<item key="page" value="${page}" />
	</param>

	<param key="video-msg-post-key">
		<item key="device" value="1" />
		<item key="thread" value="${thread-id}" />
		<item key="block_no" value="${block-no}" />
		<item key="version" value="1" />
		<item key="version_sub" value="2" />
		<item key="yugi" value="" />
	</param>

	<param key="video-dmc">
		<item key="_format" value="xml" />
		<item key="suppress_response_codes" value="true" />
	</param>

	<param key="video-dmc-reload">
		<item key="_format" value="xml" />
		<item key="_method" value="${method}" />
		<item key="suppress_response_codes" value="true" />
	</param>

	<param key="video-search">
		<item key="mode" value="watch" />
		<item key="page" value="${page}" />
		<item key="sort" value="${sort}" />
		<item key="order" value="${order}" />
	</param>

</parameters>