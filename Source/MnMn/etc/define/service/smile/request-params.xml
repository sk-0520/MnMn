		<item key="next_url" value="" />
	</param>
	<param key="smile-video-mylist-account-default-add">
		<item key="item_id" value="${video-id}" />
		<item key="token" value="${token}" />
		<item key="unwatched" value="0" />
	</param>
	<param key="smile-video-mylist-account-default-delete">
		<item key="id_list[0][]" value="${item-id-list}" />
		<item key="token" value="${token}" />
	</param>
	<param key="smile-video-mylist-account-add">
		<item key="group_id" value="${mylist-id}" />
