<?xml version="1.0" encoding="utf-8" ?>
<parameters>
	<param key="video-session-login">
		<item key="site" value="niconico" />
	</param>
	<param key="smile-user-page">
		<item key="user-id" value="${user-id}" />
	</param>

	<param key="smile-market-video-relation">
		<item key="v" value="${video-id}" />
	</param>

	<param key="smile-pedia-word-article">
		<item key="@word" value="${word}" />
	</param>

	<param key="smile-contents-search">
		<item key="service" value="${service}" />

		<item key="q" value="${query}" />
		<item key="targets" value="${targets}" />
		<item key="fields" value="${fields}" />
		<item key="filters" value="${filters}" force="true"  />
		<item key="_sort" value="${sort}" encode="uri" />
		<item key="_offset" value="${offset}" />
		<item key="_limit" value="${limit}" />
		<item key="_context" value="${context}" />
	</param>

	<param key="smile-video-mylist-user">
		<item prefix="/" key="" value="${mylist-id}" />
		<item prefix="?" key="rss" bond="=" value="2.0" />
	</param>

	<param key="smile-video-mylist-search">
		<item key="" value="/" />
		<item key="" value="${query}" encode="uri" />
		<item prefix="?" key="page" bond="=" value="${page}" />
		<item prefix="&amp;" key="sort" bond="=" value="c" />
		<item prefix="&amp;" key="order" bond="=" value="d" />
	</param>

</parameters>